<section><h2>Pedido de Transferência</h2></section>
<%= form_for(@find) do |f| %>
<body>
        <section>
            <article>
                <fieldset>
                    <p>
                       <p><%= f.label :Turno %></p>
                       <p><%= f.text_field :TURNO%></p>
                       <p><%= f.label :Número_de_pasta %></p>
                       <p><%= f.text_field :N_PASTA%></p> 
                          <button>
                            <%= image_submit_tag("botao_proximo.png")%>
                          </button>
                    </p>
                </fieldset>
            </article>    
        </section>
    </body>
<% end %>    